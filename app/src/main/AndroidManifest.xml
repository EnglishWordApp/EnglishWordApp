<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elifakay.englishwordapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logomavi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FirstActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:screenOrientation="portrait"/>
        <activity android:name=".RegisterActivity" android:screenOrientation="portrait"/>
        <activity
            android:name=".HomeActivity" android:screenOrientation="portrait"
            android:label="@string/title_activity_home" />
        <activity android:name=".PlayActivity" android:screenOrientation="portrait"/>
        <activity android:name=".DoneActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ChangePasswordActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>